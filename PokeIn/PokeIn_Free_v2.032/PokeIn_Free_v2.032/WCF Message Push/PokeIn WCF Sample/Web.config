            <error statusCode="404" redirect="FileNotFound.htm" />
        </customErrors>
        -->
	</system.web>
	<applicationSettings>
		<PokeIn_WCF_Sample.Properties.Settings>
			<setting name="PokeIn_WCF_Sample_localhost_PokeInWCF" serializeAs="String">
