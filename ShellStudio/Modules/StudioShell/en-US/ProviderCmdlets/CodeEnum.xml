    </maml:alertSet>
    <!-- Example section  -->
    <command:examples>
      <command:example>
        <maml:title>
              -------------------------- EXAMPLE 1 --------------------------
            </maml:title>
        <maml:introduction>
          <maml:para>DTE:\Solution\CodeModel\MyProject\Program.cs\MyEnum&gt;</maml:para>
        </maml:introduction>
        <dev:code>
              New-Item -Type variable -Name FirstValue -Value 1
            </dev:code>
        <dev:remarks>
          <maml:para>
                This command adds a value to the MyEnum enumeration with the name FirstValue and assigns it a value of 1.
              </maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
          <maml:para></maml:para>
        </dev:remarks>
        <command:commandLines>
